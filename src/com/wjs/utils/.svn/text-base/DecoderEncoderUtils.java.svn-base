package com.wjs.utils;

public class DecoderEncoderUtils 
{
	public static String encodeFilepath(String str)
    {
   	   return str.replaceAll("\\\\","wangjs").replaceAll("/","wangjs").replaceAll( ":","wsheng");
    }
	public static String decodeFilepath(String str)
    {
   	 return str.replaceAll("wangjs","\\\\").replaceAll( "wangjs","/").replaceAll( "wsheng",":");
    }
}
